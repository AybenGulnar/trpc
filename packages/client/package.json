{
  "name": "@trpc/client",
  "version": "10.0.0-alpha.41",
  "description": "tRPC Client lib",
  "author": "KATT",
  "license": "MIT",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/client"
  },
  "eslintConfig": {
    "rules": {
      "no-restricted-imports": [
        "error",
        "@trpc/client"
      ]
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./links/httpLink": {
      "import": "./dist/esm/links/httpLink.js",
      "require": "./dist/cjs/links/httpLink.js",
      "default": "./dist/cjs/links/httpLink.js"
    },
    "./links/httpBatchLink": {
      "import": "./dist/esm/links/httpBatchLink.js",
      "require": "./dist/cjs/links/httpBatchLink.js",
      "default": "./dist/cjs/links/httpBatchLink.js"
    },
    "./links/splitLink": {
      "import": "./dist/esm/links/splitLink.js",
      "require": "./dist/cjs/links/splitLink.js",
      "default": "./dist/cjs/links/splitLink.js"
    },
    "./links/loggerLink": {
      "import": "./dist/esm/links/loggerLink.js",
      "require": "./dist/cjs/links/loggerLink.js",
      "default": "./dist/cjs/links/loggerLink.js"
    },
    "./links/wsLink": {
      "import": "./dist/esm/links/wsLink.js",
      "require": "./dist/cjs/links/wsLink.js",
      "default": "./dist/cjs/links/wsLink.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "links",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "ts-watch": "tsc --project tsconfig.watch.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.0"
  },
  "peerDependencies": {
    "@trpc/server": "^9.12.0"
  },
  "devDependencies": {
    "@trpc/server": "^10.0.0-alpha.41"
  },
  "publishConfig": {
    "access": "public"
  }
}
