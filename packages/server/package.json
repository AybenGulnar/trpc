{
  "name": "@trpc/server",
  "version": "10.0.0-alpha.41",
  "description": "tRPC Server",
  "author": "KATT",
  "license": "MIT",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/server"
  },
  "eslintConfig": {
    "rules": {
      "explicit-module-boundary-types": "off",
      "no-restricted-imports": [
        "error",
        "@trpc/server"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "benchmark": "yarn tsc --project tsconfig.benchmark.json",
    "ts-watch": "tsc --project tsconfig.watch.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typea/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./adapters/aws-lambda": {
      "import": "./dist/esm/adapters/aws-lambda/index.js",
      "require": "./dist/cjs/adapters/aws-lambda/index.js",
      "default": "./dist/cjs/adapters/aws-lambda/index.js"
    },
    "./adapters/express": {
      "import": "./dist/esm/adapters/express.js",
      "require": "./dist/cjs/adapters/express.js",
      "default": "./dist/cjs/adapters/express.js"
    },
    "./adapters/next": {
      "import": "./dist/esm/adapters/next.js",
      "require": "./dist/cjs/adapters/next.js",
      "default": "./dist/cjs/adapters/next.js"
    },
    "./adapters/node-http": {
      "import": "./dist/esm/adapters/node-http/index.js",
      "require": "./dist/cjs/adapters/node-http/index.js",
      "default": "./dist/cjs/adapters/node-http/index.js"
    },
    "./adapters/standalone": {
      "import": "./dist/esm/adapters/standalone.js",
      "require": "./dist/cjs/adapters/standalone.js",
      "default": "./dist/cjs/adapters/standalone.js"
    },
    "./adapters/fastify": {
      "import": "./dist/esm/adapters/fastify/index.js",
      "require": "./dist/cjs/adapters/fastify/index.js",
      "default": "./dist/cjs/adapters/fastify/index.js"
    },
    "./adapters/ws": {
      "import": "./dist/esm/adapters/ws.js",
      "require": "./dist/cjs/adapters/ws.js",
      "default": "./dist/cjs/adapters/ws.js"
    },
    "./adapters/fetch": {
      "import": "./dist/esm/adapters/fetch/index.js",
      "require": "./dist/cjs/adapters/fetch/index.js",
      "default": "./dist/adapters/fetch/index.js"
    },
    "./rpc": {
      "import": "./dist/esm/rpc/index.js",
      "require": "./dist/cjs/rpc/index.js",
      "default": "./dist/cjs/rpc/index.js"
    },
    "./observable": {
      "import": "./dist/esm/observable/index.js",
      "require": "./dist/cjs/observable/index.js",
      "default": "./dist/cjs/observable/index.js"
    },
    "./subscription": {
      "import": "./dist/esm/subscription.js",
      "require": "./dist/cjs/subscription.js",
      "default": "./dist/cjs/subscription.js"
    },
    "./shared": {
      "import": "./dist/esm/shared/index.js",
      "require": "./dist/cjs/shared/index.js",
      "default": "./dist/cjs/shared/index.js"
    }
  },
  "files": [
    "dist/**",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@fastify/websocket": "^5.0.0",
    "@miniflare/core": "^2.4.0",
    "@types/aws-lambda": "^8.10.97",
    "@types/express": "^4.17.12",
    "@types/hash-sum": "^1.0.0",
    "@types/ws": "^8.2.0",
    "abort-controller": "^3.0.0",
    "aws-lambda": "^1.0.7",
    "devalue": "^2.0.1",
    "esbuild": "^0.14.38",
    "express": "^4.17.1",
    "fastify": "^3.27.1",
    "fastify-plugin": "^3.0.1",
    "fastify-websocket": "^4.0.0",
    "hash-sum": "^2.0.0",
    "jest": "^27.1.0",
    "jest-environment-miniflare": "^2.4.0",
    "konn": "^0.7.0",
    "miniflare": "^2.4.0",
    "myzod": "^1.3.1",
    "next": "^12.1.6",
    "node-fetch": "^2.6.1",
    "superstruct": "^0.15.2",
    "typescript": "4.7.4",
    "ws": "^8.0.0",
    "yup": "^0.32.8",
    "zod": "^3.0.0"
  },
  "dependencies": {
    "tslib": "^2.1.0"
  }
}
